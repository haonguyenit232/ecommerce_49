<%= form_for @category do |f| %>
  <%= render "shared/error_messages", object: f.object %>
  <%= f.label :name, t(".label_name") %>
  <%= f.text_field :name %>

  <%= f.label :parent_id, t(".parent_id") %>
  <%= f.text_field :parent_id %>

  <%= f.submit t("create_btn"), class: "btn btn-primary" %>
<% end %>
